                //    })
                //    return false;
                //});
                $('#txtCpf').mask('000.000.000-00');
                $("#btnEnviar").click(function () {
                    $(".error").fadeOut('fast');

                    if (!$("#ckbRegistrar:checked").length) {
                        $("#msgErro .msgContent p").text("Você precisa aceitar o contrato para prosseguir");
                        $("#msgErro .msgContent div").hide();
                        $("#msgErro .msgContent > a").show();
                        $(".voltar").hide();
                        $(".voltar1").show();
                        $(".continuar").hide()
                        $("#msgErro").fadeIn();
                        return false;
                    }
                    else if (!validaCPF($('#txtCpf').val().replace('.', '').replace('.', '').replace('-', ''))) {
                        $("div#msgErro .msgContent p").text("O CPF NÃO ESTÁ HABILITADO PARA A COMPRA NESTE MOMENTO.");
                        $("#msgErro .msgContent > a").show();
                        $("#msgErro .msgContent div").hide();
                        $(".voltar").hide();
                        $(".voltar1").show();
                        $(".continuar").hide();
                        $("div#msgErro").fadeIn();
                        return false;
                    }
                    else {
                        $.ajax({
                            type: "POST",
                            url: "Abbott_Login.aspx/ConsultarCPF",
                            data: "{dadosCPF:'" + $("#txtCpf").val() + "'}",
                            dataType: "json",
                            contentType: 'application/json; charset=utf-8',
                            success: function (retorno) {
                                if (retorno.d == "Ativo") {
                                    $(".voltar1").click();
                                    $("input[name='txtSenha']").val("");
                                    $("#msgErro .msgContent p").text("");
                                    $("#msgErro .msgContent div").show();
                                    $("#msgErro .msgContent > a").hide();
                                    //$(".voltar").hide();
                                    //$(".voltar1").hide();
                                    //$(".continuar").hide();
                                    $("#btnValidarSenha").attr("Style", "background: url('img/Abbott/btnEnviar.png') no-repeat;    margin-left: 12px;    left: 53%;    float: left;    position: absolute;   color: #FFF;    cursor: pointer;    text-transform: uppercase;   bottom: 40px;    border: none;    width: 107px;    height: 43px;    text-indent: 0px !important;")
                                    //$(".voltar").attr("Style", " background: url('img/Abbott/btnEnviar.png') no-repeat; margin-left: 12px; text-transform: uppercase;width: 107px;height: 43px;display: block; color: #FFF;text-decoration: none;line-height: 43px;text-indent: 26px;margin-left: -53px;left: 15%;bottom: 40px;position: absolute;font-size: 14px;");
                                    $("#btnEsqueciSenha").show();
                                    $("#btnEsqueciSenha").attr("Style", "margin-left: 12px;    background: #CF4520;   -moz-border-radius: 3px;	-webkit-border-radius: 3px;	border-radius: 3px;	    -moz-box-shadow: 0px 6px 0px #000000;    -webkit-box-shadow: 0px 6px 0px #000000;    box-shadow: 0px 2px 0px #000000;    text-transform: uppercase;    width: 135px;    height: 43px;    display: block;    color: #FFF;    text-decoration: none;    line-height: 43px;    text-indent: 8px;    margin-left: -53px;    left: 34%;    bottom: 40px;    position: absolute;    font-size: 10px;    text-align: -webkit-auto;");
                                    $(".quero-registrar").hide();
                                    $(document).keypress(function (e) {
                                        if (e.which == 13) {
                                            $("#btnValidarSenha").click();
                                        }
                                    });
                                    $("#msgErro").fadeIn();
                                }
                                else if (retorno.d == "Desativado") {
                                    $("div#msgErro .msgContent p").text("Para efetuar a sua compra é necessario fazer o cadastro no site da Drogaria Onofre");
                                    $("#msgErro .msgContent > a").show();
                                    $("#msgErro .msgContent div").hide();
                                    $(".voltar").hide();
                                    $(".voltar1").hide();
                                    $(".continuar").show();
                                    $("div#msgErro").fadeIn();
                                    //setTimeout(function () {
                                    //    window.location.href = "/Abbott_Cadastro.aspx";
                                    //}, 4000);
                                }
                                else {
                                    //$("div#msgErro .msgContent p").text("CPF não está cadastrado no site FreeStyle Libre");
                                    //$("div#msgErro .msgContent p").hide();
                                    //$("div#msgErro .msgContent").add('<p>O CPF NÃO ESTÁ HABILITADO PARA A COMPRA NESTE MOMENTO.</p>').appendTo(".msgContent");
                                    //$("#msgErro .msgContent > a").show();
                                    //$("#msgErro .msgContent div").hide();
                                    //$(".voltar").hide();
                                    //$(".voltar1").show();
                                    //$(".continuar").hide();
                                    //$("div#msgErro").fadeIn();

                                    $("div#msgErro .msgContent p").text("O CPF NÃO ESTÁ HABILITADO PARA A COMPRA NESTE MOMENTO.");
                                    $("#msgErro .msgContent > a").show();
                                    $("#msgErro .msgContent div").hide();
                                    $(".voltar").hide();
                                    $(".voltar1").show();
                                    $(".continuar").hide();
                                    $("div#msgErro").fadeIn();
                                }

                            },
                            error: function (XMLHttpRequest, textStatus, errorThrown) {
                                $("div#msgErro .msgContent p").text("Error");
                                $("#msgErro .msgContent > a").show();
                                $("#msgErro .msgContent div").hide();
                                $("div#msgErro").fadeIn();
                                $(".voltar").hide();
                                $(".continuar").hide();
                            }
                        });

                        return false;
                    }
                });

                function validaCPF(cpf) {
                    var numeros, digitos, soma, i, resultado, digitos_iguais;
                    digitos_iguais = 1;
                    if (cpf.length < 11) {
                        return false;
                    }
                    for (i = 0; i < cpf.length - 1; i++) {
                        if (cpf.charAt(i) != cpf.charAt(i + 1)) {
                            digitos_iguais = 0;
                            break;
                        }
                    }

                    if (!digitos_iguais) {
                        numeros = cpf.substring(0, 9);
                        digitos = cpf.substring(9);
                        soma = 0;
                        for (i = 10; i > 1; i--) {
                            soma += numeros.charAt(10 - i) * i;
                        }
                        resultado = soma % 11 < 2 ? 0 : 11 - soma % 11;
                        if (resultado != digitos.charAt(0)) {
                            return false;
                        }
                        numeros = cpf.substring(0, 10);
                        soma = 0;
                        for (i = 11; i > 1; i--) {
                            soma += numeros.charAt(11 - i) * i;
                        }
                        resultado = soma % 11 < 2 ? 0 : 11 - soma % 11;
                        if (resultado != digitos.charAt(1)) {
                            return false;
                        }
                        return true;
                    } else {
                        return false;
                    }
                }
            });
