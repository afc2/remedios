// initialize slideshow (Cycle)
$(document).ready(function() {
	
	var links = $('a[name=banner]');
  for (var i = 0, l = links.length; i < l; i++) {
      if (links[i].hostname === window.location.hostname)
          links[i].target = "_self";
  }

  
  $('.btn_cad_news_letter').click(function(e){
		e.preventDefault();
		
		var nome = $('.news_nome').val();
		var email = $('.news_email').val();
		
		if(nome == ''){
			nome = $('.news_nome_modal').val();
		}
		
		if(email == ''){
			email = $('.news_email_modal').val();
		}
		
		$('.loader').show();
		
		$.ajax({
			url : urlBlc + '/newsletter/inserir',
			type : 'post',
			contentType : "application/x-www-form-urlencoded;charset=UTF-8",
			data : {
				nome: nome,
				email: email
			},
			success:function(data) {
				$('.footerNewsLetter').hide();
				$('.newsletter_wrong').hide();
				$('.newsletter_check').show();
				$('.footerNewsLetter-success').show();
				$('.news_span').html(data);
				$('.loader').hide();
			},
			error: function (xhr, ajaxOptions, thrownError) {
				$('.footerNewsLetter').hide();
				$('.footerNewsLetter-success').show();
				$('.newsletter_check').hide();
				$('.newsletter_wrong').show();
				$('.news_span').html(xhr.responseText);
				$('.loader').hide();
			}
		
		});
	});

	$('.btn_novo_email').click(function(e){
		e.preventDefault();
		$('.footerNewsLetter-success').hide();
		$('.footerNewsLetter').show();
		$('.news_nome').val('');
		$('.news_email').val('');
	});

	   /*  $('#modalAmbienteTeste').modal('show');  */  
});

//]]> 
