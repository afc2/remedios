            <div class="diagonal2"></div>
            <script type="text/javascript">
                $(document).ready(function(){
                    $('#enviar').click(function(){

                        if($('#nome').val() == ''){
                            alert('Digite o nome do responsável pela causa.');
                            return false;
                        }

                        if($('#email').val() == ''){
                            alert('Digite o nome do responsável pela causa.');
                            return false;
                        }

                        if($('#valor').val() == ''){
                            alert('Digite o email do responsável pela causa.');
                            return false;
                        }

                        if($('#titulo').val() == ''){
                            alert('Digite o titulo da causa.');
                            return false;
                        }

                        if($('#descricao').val() == ''){
                            alert('Digite a descrição da causa.');
                            return false;
                        }

                        $.ajax({
                            url: "https://www.saredrogarias.com.br/mobilizacao-colaborativa/ajaxEnviarIndicacao",
                            type: "POST",
                            data: {
                                nome:$('#nome').val(),
                                celular:$('#celular').val(),
                                valor:$('#valor').val(),
                                cargo:$('#cargo').val(),
                                cidade:$('#cidade').val(),
                                estado:$('#estado').val(),
                                telefone:$('#telefone').val(),
                                email:$('#email').val(),
                                titulo:$('#titulo').val(),
                                descricao:$('#descricao').val()
                            },
                            dataType: "json",
                            beforeSend: function(){
                                $('#body').prepend('<div id="loading"><img src="https://www.saredrogarias.com.br/mobilizacao-colaborativa/loja/view/temas/mobilizacao/img/load.gif" alt="loading" /></div>');
                            },
                            success: function(result){
                                $('#loading').remove();
                                if(result.result == 'success'){
                                    $('#loading').remove();
                                    alert('Causa enviada com sucesso, aguarde o retorno de nossa equipe.');
                                }else{
                                    alert(result.msgm);
                                }
                            },
                            error: function(){
                                alert('A causa não foi enviada, entre em contato.');
                                $('#loading').remove();
                            }
                        });
                    })
                })
            </script>            <footer class="wraper">
