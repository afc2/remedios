            function SalvarCRMProduto() {

                $.ajax({
                    type: "POST",
                    url: "/DetalheProduto/Default.aspx/SalvarCrm",
                    data: "{id_produto:'" + $("#cphConteudo_hf_id_produto").val() + "',UF:'" + $("#cphConteudo_slCrmUF").val() + "',CRM:'" + $("#cphConteudo_txtCrm").val() + "',IP:'" + IP + "'}",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (data) {
                        //if (data.d == "Salvo") {


                        //} else if (data.d == "Não Salvo") {

                        //}

                    },
                    error: function (data) {

                    }
                });

            }

        </script>
    </div>
