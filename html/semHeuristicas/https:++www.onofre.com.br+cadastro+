        //Altera o campo de IE para isento
        var validador = document.getElementById("cphConteudo_CheckBoxIsento");
        var campoRG = document.getElementById("cphConteudo_txtRG");
        var labelCPF_CNPJ = document.getElementById("cphConteudo_lblCPF").innerText;
        var campoCPF_CNPJ = document.getElementById("cphConteudo_txtCPF_CNPJ").value;
        var urlEditar = window.location.href;
        
                  
         
                
        if (labelCPF_CNPJ == "CPF:" && (urlEditar.indexOf("editar") != -1)) {
            document.getElementById("cphConteudo_txtCPF_CNPJ").style = "display:none";
