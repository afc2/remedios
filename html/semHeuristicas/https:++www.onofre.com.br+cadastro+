    <script type="text/javascript">

        function BuscarCEP() {
            var CEP = $('#cphConteudo_txtCep').val();
            var CEP1 = $('#cphConteudo_txtCep1').val();

            $.ajax({
                type: "POST",
                url: "/Default.aspx/RetornoCEP",
                data: "{CEP:'" + CEP + "',CEP1:'" + CEP1 + "'}",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (data) {
                    if (data.d.Rua != null) {


                        $('#cphConteudo_txtEndereco').val(data.d.Rua);
                        $('#cphConteudo_txtBairro').val(data.d.Bairro);
                        $('#cphConteudo_txtCidade').val(data.d.Cidade);
                        $('#cphConteudo_ddlEstado').val(data.d.Estado);
                    }
                    else {
                        alert('Não foi possivel encontrar o endereço no CEP digitado. Por favor tente novamente.');
                    }

                },
                error: function (data) {
                    alert('Não foi possivel encontrar o endereço no CEP digitado. Por favor tente novamente.');

                }
            });

        }
    </script>
